{
    "name": "http-bat-vscode",
    "displayName": "http-bat",
    "version": "0.1.1",
    "publisher": "menduz",
    "description": "http-bat extension for visual studio code",
    "author": {
        "name": "menduz",
        "email": "agustin.mendez@mulesoft.com",
        "url": "http://www.menduz.com"
    },
    "categories": [
        "Languages"
    ],
    "license": "SEE LICENSE IN LICENSE.txt",
    "icon": "images/icon.svg",
